@using EFCore.Models;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
</head>
<body>
	<form action="../customiz/customizAction!findThirdKind" method="post">
		<table width="100%">
			<tr>
				<td>
					<font color="black">
						您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--III级机构设置
					</font>
				</td>
			</tr>
			<tr>
				<td align="right">
					<input type="button" value="添加" class="btn btn-primary updatePage"
						   data-url="@Url.Action("Third_kind_register","CFTKs")" />
				</td>
			</tr>
		</table>
		<table width="100%" border="1" cellpadding=0 cellspacing=1
			   bordercolorlight=#848284 bordercolordark=#eeeeee
			   class="table">
			<tr>
				<td width="12%" class="TD_STYLE1">
					I级机构名称
				</td>
				<td width="12%" class="TD_STYLE1">
					II级机构名称
				</td>
				<td width="18%" class="TD_STYLE1">
					II级机构编号
				</td>
				<td width="13%" class="TD_STYLE1">
					III级机构名称
				</td>
				<td width="15%" class="TD_STYLE1">
					销售责任人编号
				</td>
				<td width="10%" class="TD_STYLE1">
					是否为零售店
				</td>
				<td width="10%" class="TD_STYLE1">
					变更
				</td>
				<td width="10%" class="TD_STYLE1">
					删除
				</td>
			</tr>
			@foreach (var a in (List<CFTKs>)ViewData["cftks"])
			{
				<tr>
					<td class="TD_STYLE2">
						@a.FirstKindName
					</td>
					<td class="TD_STYLE2">
						@a.SecondKindName
					</td>
					<td class="TD_STYLE2">
						@a.ThirdKindId
					</td>
					<td class="TD_STYLE2">
						@a.ThirdKindName
					</td>
					<td class="TD_STYLE2">
						@a.ThirdKindSaleId
					</td>
					<td class="TD_STYLE2">
						@if(@a.ThirdKindIsRetail==1){
						<a>是</a>
						}else{
						<a>否</a>
						}
					</td>
					<td class="TD_STYLE2">
						<button type="button" class="btn btn-info refresh-link" data-url="@Url.Action("Third_kind_change","CFTKs", new{ ftkId = a.FtkId } )">变更</button>
					</td>
					<td class="TD_STYLE2">
						<button type="button" class="btn btn-warning delete-link" data-url="@Url.Action("Third_kind_delete","CFTKs", new{ ftkId = a.FtkId } )">删除</button>
					</td>
				</tr>
			}
		</table>
		<th colspan="7" class="pager">
		</th>    <script src="~/js/updatego.js"></script>

	</form>
</body>
</html>

