@{
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
</head>
<body>
	<form method="post" action="../customiz/customizAction!findPublicChar">
		<table width="100%">
			<tr>
				<td>
					<font color="black">
						您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--公共属性设置
					</font>
				</td>
			</tr>
			<tr>
				<td align="right">
					<input type="button" value="添加" class="BUTTON_STYLE1 updatePage"
						   data-url="/CPCs/Public_char_Add">
				</td>
			</tr>
		</table>
		<table width="100%" border="1" cellpadding=0 cellspacing=1
			   bordercolorlight=#848284 bordercolordark=#eeeeee
			   class="TABLE_STYLE1">
			   <tr>
					<td width="49%" class="TD_STYLE1">
						属性种类
					</td>
					<td width="44%" class="TD_STYLE1">
						属性名称
					</td>
					<td width="7%" class="TD_STYLE1" align="center">
						操作
					</td>
				</tr>
				@foreach (var list in (List<CPCs>)ViewData["list"])
			{
		
			<tr>
				<td class="TD_STYLE2">
						@list.AttributeKind
				</td>
				<td class="TD_STYLE2">
					@list.AttributeName
				</td>
				<td class="TD_STYLE2" align="center">
						<a href="#" class="delete-link" data-url="/CPCs/Public_char_Delete?id=@list.PbcId">删除</a>
				</td>
			</tr>
			}
		</table>
		<p>
			<script src="~/js/updatego.js"></script>

	</form>
</body>
</html>
