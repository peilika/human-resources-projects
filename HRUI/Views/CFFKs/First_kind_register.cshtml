@using EFCore.Models;
@model CFFKs;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
	var isok = ViewData["isOk"];
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
</head>
<body>
	<div id="First_kind_register">
		<form id="submitForm">
			<table width="100%">
				<tr>
					<td>
						<font color="black">
							您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--I级机构设置--I级机构添加
						</font>
					</td>
				</tr>
				<tr>
					<td align="right">
						<input type="button" value="提交" id="submitButton" data-url="@Url.Action("First_kind_register_success","CFFKs")" class="btn btn-primary">
						<input type="button" value="返回" class="btn btn-secondary updatePage"
							   data-url="@Url.Action("First_kind", "CFFKs")">
					</td>
				</tr>
			</table>

			<table width="100%" border="1" cellpadding=0 cellspacing=1
				   bordercolorlight=#848284 bordercolordark=#eeeeee
				   class="table">
				<tr>
					<td width="19%" class="TD_STYLE1">
					
					</td>
					<td width="81%" class="TD_STYLE2"></td>
				</tr>
				<tr>
					<td class="TD_STYLE1">
						I级机构名称
					</td>
					<td class="TD_STYLE2">
						@Html.TextBoxFor(m => m.FirstKindName, new { @class = "form-control" })
					</td>
				</tr>
				<tr>
					<td class="TD_STYLE1">
						薪酬发放责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
					</td>
					<td class="TD_STYLE2">
						@Html.TextAreaFor(m => m.FirstKindSalaryId, new { @class = "form-control", row = "4" })
					</td>
				</tr>
				<tr>
					<td class="TD_STYLE1">
						销售责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
					</td>
					<td class="TD_STYLE2">
						@Html.TextAreaFor(m => m.FirstKindSaleId, new { @class = "form-control", row = "4" })

					</td>
				</tr>
			</table>    <script src="~/js/updatego.js"></script>

		</form>
	</div>
	<script>
		$(function () {
			if ("@isok" == "unok") {
				alert("添加失败")
			}
		})
    </script>
</body>
</html>
