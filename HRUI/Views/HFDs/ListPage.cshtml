@{
	var list = ViewData["list"] as string;
}
<div id="ListPage">
	<form action="list.html" method="post">
		<font color="black"> 您在做的业务是-人力资源-调动管理-调动查询列表 </font>
		<br /><br /><br />
		<table width="100%" border="1" cellpadding=0 cellspacing=1
			   bordercolorlight=#848284 bordercolordark=#eeeeee
			   class="table">
			<tr height="21">
				<td width="20%" class="TD_STYLE1">
					I级机构
				</td>
				<td width="20%" class="TD_STYLE1">
					II级机构
				</td>
				<td width="20%" class="TD_STYLE1">
					Ⅲ级机构
				</td>
				<td width="20%" class="TD_STYLE1">
					薪酬标准
				</td>
				<td width="20%" class="TD_STYLE1">
					名字
				</td>
				@* <td width="5%" class="TD_STYLE1">
					查看
				</td> *@
			</tr>
			<tr height="21" v-for="item in tableData">
				<td class="TD_STYLE2">
					{{ item.FirstKindName }}
				</td>
				<td class="TD_STYLE2">
					{{ item.SecondKindName }}
				</td>
				<td class="TD_STYLE2">
					{{ item.ThirdKindName }}
				</td>
				<td class="TD_STYLE2">
					{{ item.SalaryStandardName }}
				</td>
				<td class="TD_STYLE2">
					{{ item.HumanName }}
				</td>
				@* <td class="TD_STYLE2">
					<input type="button" value="查看" class="btn btn-info"/>
				</td> *@
			</tr>
		</table>
	</form>
</div>
<script>
	new Vue({
		el: '#ListPage',
		data: {
			tableData: [],	// 表数据
		},
		methods: {
			/**
			 * 将json的表数据转换为数组对象
			 */
			parseJson(){
				var a = '@list'.replace(/&quot;/g, '"')
				this.tableData = JSON.parse(a);
				console.log(this.tableData)
			}
		},
		mounted() {
			this.parseJson();
		}
	})
</script>