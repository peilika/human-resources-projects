@using EFCore.Models;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
	var cfsks = ViewData["cfsks"] as List<CFSKs>;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
</head>
<body>
	<form action="../customiz/customizAction!findSecondKind" method="post">
		<table width="100%">
			<tr>
				<td>
					<font color="black">
						您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--II级机构设置
					</font>
				</td>
			</tr>
			<tr>
				<td align="right">
					<input type="button" value="添加" class="btn btn-primary updatePage"
						   data-url="@Url.Action("Second_kind_register","CFSKs")" />
				</td>
			</tr>
		</table>
		<table width="100%" border="1" cellpadding=0 cellspacing=1
			   bordercolorlight=#848284 bordercolordark=#eeeeee
			   class="table">
			<tr>
				<td width="15%" class="TD_STYLE1">
					I级机构名称
				</td>
				<td width="12%" class="TD_STYLE1">
					II级机构编号
				</td>
				<td width="12%" class="TD_STYLE1">
					II级机构名称
				</td>
				<td width="20%" class="TD_STYLE1">
					薪酬发放责任人编号
				</td>
				<td width="20%" class="TD_STYLE1">
					销售责任人编号
				</td>
				<td width="10%" class="TD_STYLE1">
					变更
				</td>
				<td width="10%" class="TD_STYLE1">
					删除
				</td>
			</tr>
			@foreach (var a in cfsks)
			{
				<tr>
					<td class="TD_STYLE2">
						@a.FirstKindName
					</td>
					<td class="TD_STYLE2">
						@a.SecondKindId
					</td>
					<td class="TD_STYLE2">
						@a.SecondKindName
					</td>
					<td class="TD_STYLE2">
						@a.SecondSalaryId
					</td>
					<td class="TD_STYLE2">
						@a.SecondSaleId
					</td>
					<td class="TD_STYLE2">
						<button type="button" class="refresh-link btn btn-info" data-url="@Url.Action("Second_kind_change","CFSKs", new{ fskId = a.FskId } )">变更</button>
					</td>
					<td class="TD_STYLE2">
						<button type="button" class="delete-link btn btn-warning" data-url="@Url.Action("Second_kind_delete","CFSKs", new{ fskId = a.FskId } )">删除</button>
					</td>
				</tr>
			}
			
		</table>
		<br><br>
		<th colspan="7" class="pager">
		</th>
		<p>    <script src="~/js/updatego.js"></script>

	</form>
	<script type="text/javascript">

		function submission() {
			document.forms[0].submit();
		}

		function page(currentPage) {
			document.getElementById("pid").value = currentPage;
			document.forms[0].submit();
		}
    </script>
</body>
</html>

