@using EFCore.Models;
@model CFSKs;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
	var isok = ViewData["isOk"] as string;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
</head>
<body>
	<form id="submitForm">

		<table width="100%">
			<tr>
				<td>
					<font color="black">
						您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--II级机构设置--II级机构变更
					</font>
				</td>
			</tr>
			<tr>
				<td align="right">
					<input type="button" value="提交" class="btn btn-primary" id="submitButton" data-url="@Url.Action("Second_kind_change_success","CFSKs")">
					<input type="button" value="返回" class="btn btn-info updatePage"
						   data-url="@Url.Action("Second_kind","CFSKs")">
				</td>
			</tr>
		</table>

		<table width="100%" border="1" cellpadding=0 cellspacing=1
			   bordercolorlight=#848284 bordercolordark=#eeeeee
			   class="table">
			<input type="hidden" name="cfsk.fskId" value=761 class="INPUT_STYLE1" />
			@Html.TextBoxFor(m => m.FskId, new { @class = "INPUT_STYLE1", @readonly="readonly", style="display:none;" })
			<tr>
				<td width="19%" class="TD_STYLE1">
					I级机构名称
				</td>
				<td width="81%" class="TD_STYLE2">
					@Html.TextBoxFor(m => m.FirstKindName, new { @class = "form-control", @readonly="readonly" })
				</td>
			</tr>
			<tr>
				<td width="19%" class="TD_STYLE1">
					II级机构编号
				</td>
				<td width="81%" class="TD_STYLE2">
					@Html.TextBoxFor(m => m.SecondKindId, new { @class = "form-control", @readonly="readonly" })
				</td>
			</tr>
			<tr>
				<td class="TD_STYLE1">
					II级机构名称
				</td>
				<td class="TD_STYLE2">
					@Html.TextBoxFor(m => m.SecondKindName, new { @class = "form-control", @readonly="readonly" })
				</td>
			</tr>
			<tr>
				<td class="TD_STYLE1">
					薪酬发放责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
				</td>
				<td class="TD_STYLE2">
					@Html.TextAreaFor(m => m.SecondSalaryId, new { @class = "form-control", row = "4" })
				</td>
			</tr>
			<tr>
				<td class="TD_STYLE1">
					销售责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
				</td>
				<td class="TD_STYLE2">
					@Html.TextAreaFor(m => m.SecondSaleId, new { @class = "form-control", row = "4" })
				</td>
			</tr>
		</table>    <script src="~/js/updatego.js"></script>

	</form>
	<script>
		$(function () {
			if ("@isok" == "unOk") {
				alert("修改失败")
			}
		})
		//控件判断不能为空
		function check() {
			if (document.getElementById("secondKind.secondSalaryId").value == "") {
				alert("薪酬发放责任人编号不能为空!!!");
				return false;
			}
			if (document.getElementById("secondKind.secondSaleId").value == "") {
				alert("销售责任人编号不能为空!!!");
				return false;
			}
			document.forms[0].submit();
		}
    </script>
</body>
</html>
