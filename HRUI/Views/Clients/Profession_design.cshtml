<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-trasitional.dtd">
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
</head>
<body>
	<form method="post" action="../customiz/customizAction!findAttribute">
		<table width="100%">
			<tr>
				<td>
					<font color="black">
						您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--职称设置
					</font>
				</td>
			</tr>
			<tr>
				<td align="right">
				</td>
			</tr>
		</table>
		<table width="100%" border="1" cellpadding=0 cellspacing=1
			   bordercolorlight=#848284 bordercolordark=#eeeeee
			   class="TABLE_STYLE1">
			<tr>
				<td width="80%" class="TD_STYLE1">
					职称名称
				</td>
				<td width="20%" class="TD_STYLE1" align="center">
					删除
				</td>
			</tr>

			@foreach(var client in ViewData["list"] as List<CPCs>){
				@if (client.AttributeKind == "职称设置")
				{	
					<tr>
						<td class="TD_STYLE2">
							@client.AttributeName
						</td>
						<td class="TD_STYLE2" align="center">
							<a data-url="/Clients/ClientDelete?id=@client.PbcId">删除</a>
						</td>
					</tr>
				}
			}



		</table>
		<p>
			<script src="~/js/updatego.js"></script>

	</form>
</body>
</html>

